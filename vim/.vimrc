syntax on
set number
set termguicolors

set showtabline=2
set tabpagemax=15

let g:netrw_banner = 0
let g:netrw_liststyle = 3
let g:netrw_winsize = 25
let g:netrw_altv = 1
let g:netrw_browse_split = 4

let mapleader = "\<Space>"
nnoremap <leader>e :Lexplore<CR>
nnoremap <leader>l :tabnext<CR>
nnoremap <leader>h :tabprevious<CR>
